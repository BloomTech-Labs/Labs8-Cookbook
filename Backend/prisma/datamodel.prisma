type User {
  id: ID! @unique
  first_name: String! 
  last_name: String!
  email: String! @unique
  password: String!
  posts: [Post!]!
}

type Post {
  id: ID! @unique
  title: String!
  content: String!
  published: Boolean! @default(value: "false")
  author: User!
  date: String!
}

type Recipe {
  id: ID! @unique
  name: String! @unique
}